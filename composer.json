{
    "name": "ossobuffo/switch_to_minimal",
    "description": "Drush hook to switch Drupal install profile to whatever profile is configured in core.extension.yml.",
    "type": "drupal-drush",
    "license": "GPL-2.0-or-later",
    "homepage": "https://github.com/ossobuffo/switch-to-minimal",
    "authors": [
        {
            "name": "Daniel Johnson",
            "email": "il.basso.buffo@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.3.0",
        "drupal/core": "^10.2 || ^11",
        "drush/drush": ">=13.0.0"
    },
    "extra": {
        "installer-name": "switch_to_minimal",
        "installer-paths": {
            "drush/Commands/contrib/{$name}": [ "ossobuffo/switch_to_minimal" ]
        }
    }
}
